<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.campus.mapper.ExamMapper">

    <!-- 保存试卷信息 -->
    <insert id="saveExamInfo" useGeneratedKeys="true" keyProperty="examid">
        INSERT INTO `tb_exam`(`ename`, `etype`, `efenzhi`, `testid`) VALUES (#{ename},#{etype},#{efenzhi},#{testid});
    </insert>

</mapper>