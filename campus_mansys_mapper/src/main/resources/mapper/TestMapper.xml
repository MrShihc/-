<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.campus.mapper.TestMapper">

    <!-- 保存考试和班级之间的关联信息 -->
    <insert id="saveTestGradeInfo">
        	INSERT INTO `tb_test_grade`(`gid`, `testid`,tid)
        	VALUES(#{gid},#{testid},#{tid})
    </insert>

</mapper>